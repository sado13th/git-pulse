{
  "include": [
    "src"
  ],
  "exclude": [
    "**/__pycache__",
    ".venv",
    "venv",
    "build",
    "dist"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportUnusedImport": true,
  "reportUnusedVariable": true
}
